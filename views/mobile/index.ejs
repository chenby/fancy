<script type="text/javascript" >
	window.onload=function(){
		//加入购物车
		addclickevent()
		function addclickevent(){
			for (var i = 0; i < $('.add_shop').length; i++) {
				$('.add_shop')[i].ontouchend=$('.add_shop')[i].onclick=function(){
					var sp = $(this).parents('span').parents('div').prev('a').children('div').html();
					var price_str= $(this).parents('span').prev('span').children('b').html();
					console.log(sp);
					$.post('add_to_cart',{sp:sp,price:price_str},function(){
						alert('加入购物车成功');
					})	
				}
			};
		}
		
		//loading页面start
		document.getElementsByClassName('loading')[0].style.display='none';
		document.getElementsByClassName('extend_close')[0].ontouchend=document.getElementsByClassName('extend_close')[0].onclick=function(event){
		document.getElementsByClassName('extend')[0].style.display='none';
		}
		//loading页面end
		//下拉更新
		var base=0;
		 $('.add-more')[0].ontouchend=$('.add-more')[0].onclick=function(){
		 	$(this).hide();
		 	$('.loadding').show();
		 	base+=8;
		 	$.post('../goods_getten',{base:base},function(data){
		 		var data =JSON.parse(data);	
		 		 if (data.length) { 		 	
			 		$('.loadding').hide();
			 		$('.add-more').show();
			 		for (var i = 0; i < data.length; i++) {		  			 
			 			 $('#wrapper ul').append('<li><a href=""><img class="load-img" src="/images/cloth/load.jpg" date-src="/images/cloth/'+data[i].src+'.jpg" ><div class="good-name">'+data[i].name+'</div></a><div><span class="good-price">￥<b>'+data[i].price+'</b></span><span><a class="orange add_shop" href="javascripte:void(0);">加入购物车</a></span></div></li>');
			 		};
			 		load();
			 		addclickevent();
		 		 }else{
		 		 	$('.loadding').hide();
		 		 	$('.add-more').show();
		 		 	$('.add-more').text('已经没有更多了')
		 		 }
		 				 		
		 	})

		 }
		//下拉更新
		//返回顶部display
		window.onscroll=function(){
			if (document.body.scrollTop>=440||document.documentElement.scrollTop>=440) {
				document.getElementsByClassName('sroll-top')[0].style.display='block';
			}else{
				document.getElementsByClassName('sroll-top')[0].style.display='none';
			}
		}//返回顶部display none
		srcollTop('sroll-top');//返回顶部
		//返回顶部function
		function srcollTop(class_name){
			var timer=null;
			document.getElementsByClassName(class_name)[0].ontouchend=document.getElementsByClassName(class_name)[0].onclick=function(event){
				clearInterval(timer);
				 timer=setInterval(function(){
					if(document.documentElement.scrollTop>0||document.body.scrollTop>0) {
						document.documentElement.scrollTop-=100;
	 				 	document.body.scrollTop-=100;
					}else{
						console.log('message');
						clearInterval(timer);
					}
				},20);	
			}
		}
		//返回顶部end
		
	 	load( )
		function load(){
			var imgSrc=[]
			var nowImg=$('.load-img[src="/images/cloth/load.jpg"]')
			for (var i = 0; i < nowImg.length; i++) {
				imgSrc.push(nowImg[i].getAttribute('date-src'))
			};		
			preloadimages(imgSrc).done(function(newimages){
				 for (var i = 0; i < newimages.length; i++) {
				 	nowImg[i].src=newimages[i].src;
				 };
			})
		}
		//图片预加载function
		function preloadimages(arr){   
			var newimages=[], loadedimages=0;
			var postaction=function(){};
			var arr=(typeof arr!="object")? [arr] : arr;
			function imageloadpost(){
			    loadedimages++
			    if (loadedimages==arr.length){
			        postaction(newimages) //加载完成用我们调用postaction函数并将newimages数组做为参数传递进去
			    }
			}
			for (var i=0; i<arr.length; i++){
			    newimages[i]=new Image()
			    newimages[i].src=arr[i]
			    newimages[i].onload=function(){
			        imageloadpost()
			    }
			    newimages[i].onerror=function(){
			        imageloadpost()
			    }
			}
			return { //此处返回一个空白对象的done方法
			    done:function(f){
			        postaction=f || postaction
			    }
			}
		}//图片预加载function
		
	}
</script>
